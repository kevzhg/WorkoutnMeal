{
  "name": "workout-meal-tracker",
  "version": "1.0.0",
  "description": "Track daily workouts, meal plans, and weight changes",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:server": "tsc -p tsconfig.server.json",
    "build:all": "npm run build && npm run build:server",
    "watch": "tsc -w",
    "start": "npx serve . -p 3000",
    "server": "node dist-server/server.js",
    "dev": "npm run build:all && (npm run server & npm run start)"
  },
  "keywords": [
    "workout",
    "meal",
    "tracker",
    "fitness"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "serve": "^14.2.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0"
  }
}
